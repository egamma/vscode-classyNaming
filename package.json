{
	"name": "vscode-classyNaming",
	"version": "0.0.1",
	"publisher": "Andrei.Zubov",
	"engines": {
		"vscode": "*"
	},
	"activationEvents": [
		"onLanguage:csharp"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [{
			"command": "extension.sayHello",
			"title": "Hello World"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/typescript/lib/tsc.js",
		"compile": "node ./node_modules/typescript/lib/tsc.js -watch -p ./"
	},
	"devDependencies": {
		"vscode": "*"
	}
}